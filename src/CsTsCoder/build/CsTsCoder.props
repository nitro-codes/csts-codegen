<!-- 
  This file is automatically imported by NuGet into a user's project
  when it targets a single framework, or in classic (pre 2017) csproj projects.
-->
<Project TreatAsLocalProperty="TaskFolder;TaskAssembly">
  <PropertyGroup>
    <TaskFolder>netstandard2.1</TaskFolder>
    <TaskAssembly>$(MSBuildThisFileDirectory)..\lib\$(TaskFolder)\CsTsCoder.dll</TaskAssembly>
  </PropertyGroup>

  <UsingTask TaskName="CsTsCoder.CoderTask" AssemblyFile="$(TaskAssembly)" />
</Project>
